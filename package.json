{
    "name": "hull.js",
    "version": "1.0.10",
    "description": "JavaScript library that builds concave hull (shape) by set of points",
    "homepage": "https://github.com/AndriiHeonia/hull",
    "keywords": [
        "geometry",
        "concave-hull",
        "math",
        "algorithm"
    ],
    "author": "Andrii Heonia <a.heonia@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git://github.com/AndriiHeonia/hull.git"
    },
    "main": "./src/hull.js",
    "types": "./src/hull.d.ts",
    "devDependencies": {
        "nodemon": "^3.1.7",
        "mocha": "^10.8.0",
        "jshint": "^2.13.6",
        "browserify": "^17.0.1"
    },
    "scripts": {
        "watch": "./node_modules/nodemon/bin/nodemon.js --watch src --exec \"./node_modules/browserify/bin/cmd.js ./src/hull.js --standalone hull > ./dist/hull.js\"",
        "test": "./node_modules/jshint/bin/jshint src && ./node_modules/.bin/mocha -R spec && ./node_modules/browserify/bin/cmd.js ./src/hull.js --standalone hull > ./dist/hull.js"
    },
    "jshintConfig": {
        "forin": false,
        "node": true,
        "esversion": 6
    },
    "license": "BSD"
}
